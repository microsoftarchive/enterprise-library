<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!--Brushes -->
	<SolidColorBrush x:Key="PopupBackground"
					 Color="#FFF3F8FF" />

	<Color x:Key="ExpanderBackgroundColor">#FFB3B9BB</Color>

	<LinearGradientBrush x:Key="NormalBrush"
						 EndPoint="0,1"
						 StartPoint="0,0">
		<GradientStop Color="#EEE"
					  Offset="0.0" />
		<GradientStop Color="#CCC"
					  Offset="1.0" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="LightGreyBorderBrush"
					 Color="#FFBABABA" />

	<SolidColorBrush x:Key="AdornerStrokeBrush"
					 Color="#FF61A0FF" />
	<SolidColorBrush x:Key="AdornerFillBrush"
					 Color="#3F99C2FF" />
    <SolidColorBrush x:Key="ElementCommandNormalBrush" Color="#FF949494" />
    <SolidColorBrush x:Key="ElementCommandMouseOverBrush" Color="#FFB4B4B4" />
    <SolidColorBrush x:Key="ElementCommandShadowBrush" Color="#FF000000" />
    <SolidColorBrush x:Key="ElementCommandBackgroundBrush" Color="#00FFFFFF" />    
	
	<SolidColorBrush x:Key="ElementContextButtonStroke" Color="#FFB2B4B7"/>
	<SolidColorBrush x:Key="ElementContextMenuButtonStrokeHighlight" Color="#FF646464" />
	<SolidColorBrush x:Key="ElementContextMouseOverBrush" Color="#FFECCDAA" />
	
</ResourceDictionary>